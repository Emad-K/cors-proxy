{
  "name": "cors-proxy",
  "version": "1.0.0",
  "description": "A high-performance CORS proxy server with intelligent caching, rate limiting, and comprehensive logging",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node src/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/helpers.test.js tests/env.test.js tests/logger.test.js",
    "test:integration": "vitest run tests/integration.test.js tests/app.test.js",
    "test:e2e": "vitest run tests/e2e.test.js"
  },
  "keywords": [
    "cors",
    "proxy",
    "hono",
    "redis",
    "rate-limiting",
    "caching",
    "docker",
    "nodejs"
  ],
  "author": "Emad Kazemi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Emad-K/cors-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/Emad-K/cors-proxy/issues"
  },
  "homepage": "https://github.com/Emad-K/cors-proxy#readme",
  "packageManager": "pnpm@10.6.5",
  "dependencies": {
    "@hono-rate-limiter/redis": "^0.1.4",
    "@hono/node-server": "^1.19.5",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "hono": "^4.9.10",
    "hono-pino": "^0.10.3",
    "hono-rate-limiter": "^0.4.2",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.8.1",
    "pino": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "eslint-config-xo": "^0.49.0",
    "eslint-plugin-import": "^2.32.0",
    "vitest": "^3.2.4"
  }
}